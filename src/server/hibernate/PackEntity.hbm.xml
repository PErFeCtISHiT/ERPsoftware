<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="server.hibernate.PackEntity" table="PACK" schema="PIS">
        <id name="keyno">
            <column name="KEYNO" sql-type="varchar2(20)" length="20"/>
        </id>
        <property name="goodsno">
            <column name="GOODSNO" sql-type="varchar2(200)" length="200" not-null="true"/>
        </property>
        <property name="goodsname">
            <column name="GOODSNAME" sql-type="varchar2(200)" length="200" not-null="true"/>
        </property>
        <property name="price">
            <column name="PRICE" sql-type="number(*)" precision="2147483646" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>