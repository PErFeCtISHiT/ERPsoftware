<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="server.hibernate.UseEntity" table="USE" schema="PIS">
        <id name="keyno">
            <column name="KEYNO" sql-type="varchar2(20)" length="20"/>
        </id>
        <property name="keyjob">
            <column name="KEYJOB" sql-type="varchar2(20)" length="20" />
        </property>
        <property name="lev">
            <column name="LEV" sql-type="number(*)" precision="2147483646" />
        </property>
        <property name="keyname">
            <column name="KEYNAME" sql-type="varchar2(20)" length="20" />
        </property>
        <property name="tel">
            <column name="TEL" sql-type="varchar2(20)" length="20" />
        </property>
        <property name="address">
            <column name="ADDRESS" sql-type="varchar2(50)" length="50" />
        </property>
        <property name="postcode">
            <column name="POSTCODE" sql-type="varchar2(20)" length="20" />
        </property>
        <property name="email">
            <column name="EMAIL" sql-type="varchar2(50)" length="50" />
        </property>
        <property name="keyyear">
            <column name="KEYYEAR" sql-type="number(*)" precision="2147483646" />
        </property>
        <property name="passwor">
            <column name="PASSWOR" sql-type="varchar2(50)" length="50" />
        </property>
    </class>
</hibernate-mapping>