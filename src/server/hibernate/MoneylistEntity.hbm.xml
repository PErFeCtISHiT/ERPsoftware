<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="server.hibernate.MoneylistEntity" table="MONEYLIST" schema="PIS">
        <id name="keyid">
            <column name="KEYID" sql-type="varchar2(20)" length="20"/>
        </id>
        <property name="keyno">
            <column name="KEYNO" sql-type="varchar2(20)" length="20"/>
        </property>
        <property name="accountname">
            <column name="ACCOUNTNAME" sql-type="varchar2(20)" length="20" />
        </property>
        <property name="sumall">
            <column name="SUMALL" sql-type="number(*)" precision="2147483646" />
        </property>
        <property name="note">
            <column name="NOTE" sql-type="varchar2(20)" length="2000"/>
        </property>
    </class>
</hibernate-mapping>