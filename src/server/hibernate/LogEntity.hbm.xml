<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="server.hibernate.LogEntity" table="LOG" schema="PIS">
        <id name="keyno">
            <column name="KEYNO" sql-type="varchar2(20)" length="20"/>
        </id>
        <property name="adddate">
            <column name="ADDDATE" sql-type="date" />
        </property>
        <property name="keyjob">
            <column name="KEYJOB" sql-type="varchar2(20)" length="20" />
        </property>
        <property name="operatorno">
            <column name="OPERATORNO" sql-type="varchar2(20)" length="20" />
        </property>
        <property name="billno">
            <column name="BILLNO" sql-type="varchar2(20)" length="20" />
        </property>
        <property name="opno">
            <column name="OPNO" sql-type="varchar2(20)" length="20" />
        </property>
        <property name="note">
            <column name="NOTE" sql-type="varchar2(20)" length="2000" />
        </property>
        <property name="goodsname">
            <column name="GOODSNAME" sql-type="varchar2(20)" length="20" />
        </property>
        <property name="consumer">
            <column name="CONSUMER" sql-type="varchar2(20)" length="20" />
        </property>
        <property name="base">
            <column name="BASE" sql-type="varchar2(20)" length="20" />
        </property>
    </class>
</hibernate-mapping>