<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="server.hibernate.ConsumerEntity" table="CONSUMER" schema="PIS">
        <id name="keyno">
            <column name="KEYNO" sql-type="varchar2(20)" length="20"/>
        </id>
        <property name="kinds">
            <column name="KINDS" sql-type="number(*)" precision="2147483646" not-null="true"/>
        </property>
        <property name="lev">
            <column name="LEV" sql-type="number(*)" precision="2147483646" not-null="true"/>
        </property>
        <property name="keyname">
            <column name="KEYNAME" sql-type="varchar2(20)" length="20" not-null="true"/>
        </property>
        <property name="tel">
            <column name="TEL" sql-type="varchar2(20)" length="20" not-null="true"/>
        </property>
        <property name="mailcode">
            <column name="MAILCODE" sql-type="varchar2(20)" length="20" not-null="true"/>
        </property>
        <property name="email">
            <column name="EMAIL" sql-type="varchar2(40)" length="40" not-null="true"/>
        </property>
        <property name="capacit">
            <column name="CAPACIT" sql-type="number(*)" precision="2147483646" not-null="true"/>
        </property>
        <property name="receive">
            <column name="RECEIVE" sql-type="number(*)" precision="2147483646" not-null="true"/>
        </property>
        <property name="pay">
            <column name="PAY" sql-type="number(*)" precision="2147483646" not-null="true"/>
        </property>
        <property name="server">
            <column name="SERVER" sql-type="varchar2(20)" length="20" not-null="true"/>
        </property>
        <property name="accoun">
            <column name="ACCOUN" sql-type="varchar2(20)" length="20" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>