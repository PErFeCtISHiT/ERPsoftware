<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="server.hibernate.AccountInitEntity" table="ACCOUNTINIT" schema="PIS">
        <id name="keyno">
            <column name="KEYNO" sql-type="varchar2(20)" length="20"/>
        </id>
        <property name="keyyear">
            <column name="KEYYEAR" sql-type="varchar2(20)" length="20" />
        </property>
        <property name="accountlist">
            <column name="ACCOUNTLIST" sql-type="varchar2(2000)" length="2000" />
        </property>
        <property name="consumerlist">
            <column name="CONSUMERLIST" sql-type="varchar2(2000)" length="2000" />
        </property>
        <property name="goodslist">
            <column name="GOODSLIST" sql-type="varchar2(2000)" length="2000" />
        </property>
    </class>
</hibernate-mapping>